<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>TP</groupId>
  <artifactId>seleniumProject</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  
  <properties>
     <maven.complier.target>1.8</maven.complier.target>
     <maven.compiler.source>1.8</maven.compiler.source>
  </properties>
  
 <profiles>
  <profile>
    <id>ST</id>
    <build>
       <plugins>
           <plugin>
               <groupId>org.apache.maven.plugins</groupId>
               <artifactId>maven-surefire-plugin</artifactId>
               <version>3.5.3</version> <!-- Use the latest stable version -->
               <configuration>
                   <suiteXmlFiles>
                       <suiteXmlFile>smokeSuite.xml</suiteXmlFile>
                       <!-- Add more <suiteXmlFile> tags if you have multiple TestNG XML files -->
                   </suiteXmlFiles>
               </configuration>
           </plugin>
       </plugins>
   </build>
  </profile>
  
  <profile>
    <id>RT</id>
    <build>
       <plugins>
           <plugin>
               <groupId>org.apache.maven.plugins</groupId>
               <artifactId>maven-surefire-plugin</artifactId>
               <version>3.5.3</version> <!-- Use the latest stable version -->
               <configuration>
                   <suiteXmlFiles>
                       <suiteXmlFile>regressionSuite.xml</suiteXmlFile>
                       <!-- Add more <suiteXmlFile> tags if you have multiple TestNG XML files -->
                   </suiteXmlFiles>
               </configuration>
           </plugin>
       </plugins>
   </build>
  </profile>
  
  <profile>
    <id>RRT</id>
    <build>
       <plugins>
           <plugin>
               <groupId>org.apache.maven.plugins</groupId>
               <artifactId>maven-surefire-plugin</artifactId>
               <version>3.5.3</version> <!-- Use the latest stable version -->
