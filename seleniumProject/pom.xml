                       <!-- Add more <suiteXmlFile> tags if you have multiple TestNG XML files -->
                   </suiteXmlFiles>
               </configuration>
           </plugin>
       </plugins>
   </build>
  </profile>
  
  <profile>
    <id>GT</id>
    <build>
       <plugins>
           <plugin>
               <groupId>org.apache.maven.plugins</groupId>
               <artifactId>maven-surefire-plugin</artifactId>
               <version>3.5.3</version> <!-- Use the latest stable version -->
               <configuration>
                   <suiteXmlFiles>
                       <suiteXmlFile>groupsSuite.xml</suiteXmlFile>
                       <!-- Add more <suiteXmlFile> tags if you have multiple TestNG XML files -->
                   </suiteXmlFiles>
               </configuration>
           </plugin>
       </plugins>
   </build>
  </profile>
  
 </profiles>
  
  
